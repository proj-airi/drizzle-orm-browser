{
  "name": "@proj-airi/unplugin-drizzle-orm-migrations",
  "type": "module",
  "version": "0.1.2",
  "description": "ðŸ¦† Drizzle ORM migrations bundler plugin for bundling drizzle-kit migrations!",
  "author": {
    "name": "Neko Ayaka",
    "email": "neko@ayaka.moe",
    "url": "https://github.com/nekomeowww"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/proj-airi/drizzle-orm-browser.git",
    "directory": "packages/unplugin-drizzle-orm-migrations"
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./vite": "./dist/vite.mjs",
    "./webpack": "./dist/webpack.mjs",
    "./rspack": "./dist/rspack.mjs",
    "./rollup": "./dist/rollup.mjs",
    "./rolldown": "./dist/rolldown.mjs",
    "./esbuild": "./dist/esbuild.mjs",
    "./api": "./dist/api.mjs",
    "./types": {
      "types": "./dist/types.d.mts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "tsdown",
    "stub": "tsdown",
    "build": "tsdown",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "attw": "attw --pack . --profile esm-only --ignore-rules cjs-resolves-to-esm"
  },
  "peerDependencies": {
    "vite": "^2.9.0 || ^3.0.0-0 || ^4.0.0 || ^5.0.0-0 || ^6.0.0-0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "dependencies": {
    "c12": "^3.0.4",
    "uncrypto": "^0.1.3",
    "unplugin": "^2.3.5"
  }
}
