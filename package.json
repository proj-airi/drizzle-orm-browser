{
  "name": "@proj-airi/drizzle-orm-browser",
  "type": "module",
  "version": "0.1.3",
  "private": true,
  "packageManager": "pnpm@10.15.1",
  "author": {
    "name": "Neko Ayaka",
    "email": "neko@ayaka.moe",
    "url": "https://github.com/nekomeowww"
  },
  "scripts": {
    "postinstall": "pnpm run build:packages",
    "dev": "pnpm -F @proj-airi/drizzle-orm-browser-playground dev",
    "build": "pnpm -r -F=\"./packages/*\" -F=\"./playground\" run build",
    "build:packages": "pnpm -F=\"./packages/*\" run build",
    "test": "vitest --coverage",
    "test:run": "vitest run",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "typecheck": "pnpm -r -F=\"./packages/*\" -F=\"./apps/*\" run --parallel typecheck",
    "up": "taze -w -r -I -f && pnpm prune && pnpm dedupe"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.3.0",
    "@antfu/ni": "^25.0.0",
    "@types/node": "^24.3.1",
    "@unocss/eslint-config": "^66.5.1",
    "@unocss/eslint-plugin": "^66.5.1",
    "@unocss/preset-web-fonts": "^66.5.1",
    "bumpp": "^10.2.3",
    "changelogithub": "^13.16.0",
    "eslint": "^9.35.0",
    "taze": "^19.6.0",
    "tsdown": "^0.15.0",
    "tsx": "^4.20.5",
    "typescript": "~5.9.2",
    "unocss": "^66.5.1",
    "unplugin-unused": "^0.5.3",
    "vite": "^7.1.5",
    "vite-plugin-inspect": "^11.3.3",
    "vitest": "^3.2.4"
  }
}
